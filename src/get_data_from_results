#! /bin/bash

make_table () {

	texfile=$1

	file=$2
	rfile=/tmp/raoult_$(basename -- $file)
	nfile=/tmp/norrish_$(basename -- $file)
	vfile=/tmp/virial_$(basename -- $file)
	ufile=/tmp/uniquac_$(basename -- $file)
	NAME=$(basename -- $file)

	FIRST=$(head -n 1 $rfile | cut -d ',' -f 2)
	SECOND=$(head -n 1 $rfile | cut -d ',' -f 3)
	THIRD=$(head -n 1 $rfile | cut -d ',' -f 4)

	NK1=$(grep 'K_0' $nfile | grep '=' \
		| tr '\t' ' ' | cut -d ' ' -f 4)

	VB1=$(grep 'b_0' $vfile | grep '=' \
		| tr '\t' ' ' | cut -d ' ' -f 4)
	VB2=$(grep 'b_1' $vfile | grep '=' \
		| tr '\t' ' ' | cut -d ' ' -f 4)
	VB3=$(grep 'b_3' $vfile | grep '=' \
		| tr '\t' ' ' | cut -d ' ' -f 4)
	VC12=$(grep 'c_2' $vfile | grep '=' \
		| tr '\t' ' ' | cut -d ' ' -f 4)
	VC13=$(grep 'c_4' $vfile | grep '=' \
		| tr '\t' ' ' | cut -d ' ' -f 4)
	VC23=$(grep 'c_5' $vfile | grep '=' \
		| tr '\t' ' ' | cut -d ' ' -f 4)

	UA00=$(grep 'A_00' $ufile | grep '=' \
		| tr '\t' ' ' | cut -d ' ' -f 4)
	UA01=$(grep 'A_01' $ufile | grep '=' \
		| tr '\t' ' ' | cut -d ' ' -f 4)
	UA02=$(grep 'A_02' $ufile | grep '=' \
		| tr '\t' ' ' | cut -d ' ' -f 4)
	UA03=$(grep 'A_03' $ufile | grep '=' \
		| tr '\t' ' ' | cut -d ' ' -f 4)
	UA10=$(grep 'A_10' $ufile | grep '=' \
		| tr '\t' ' ' | cut -d ' ' -f 4)
	UA11=$(grep 'A_11' $ufile | grep '=' \
		| tr '\t' ' ' | cut -d ' ' -f 4)
	UA12=$(grep 'A_12' $ufile | grep '=' \
		| tr '\t' ' ' | cut -d ' ' -f 4)
	UA13=$(grep 'A_13' $ufile | grep '=' \
		| tr '\t' ' ' | cut -d ' ' -f 4)
	UA20=$(grep 'A_20' $ufile | grep '=' \
		| tr '\t' ' ' | cut -d ' ' -f 4)
	UA21=$(grep 'A_21' $ufile | grep '=' \
		| tr '\t' ' ' | cut -d ' ' -f 4)
	UA22=$(grep 'A_22' $ufile | grep '=' \
		| tr '\t' ' ' | cut -d ' ' -f 4)
	UA23=$(grep 'A_23' $ufile | grep '=' \
		| tr '\t' ' ' | cut -d ' ' -f 4)
	UA30=$(grep 'A_30' $ufile | grep '=' \
		| tr '\t' ' ' | cut -d ' ' -f 4)
	UA31=$(grep 'A_31' $ufile | grep '=' \
		| tr '\t' ' ' | cut -d ' ' -f 4)
	UA32=$(grep 'A_32' $ufile | grep '=' \
		| tr '\t' ' ' | cut -d ' ' -f 4)
	UA33=$(grep 'A_33' $ufile | grep '=' \
		| tr '\t' ' ' | cut -d ' ' -f 4)

	RPR=$(grep "coeff. of determination" $rfile \
		| grep -v "aw" \
		| grep '=' \
		| cut -d ' ' -f 10)
	RAR=$(grep "coeff. of determination (aw)" $rfile \
		| grep '=' \
		| cut -d ' ' -f 6)
	RMR=$(grep "final cost" $rfile \
		| grep '=' \
		| cut -d ' ' -f 15)

	RPN=$(grep "coeff. of determination" $nfile \
		| grep -v "aw" \
		| grep '=' \
		| cut -d ' ' -f 6)
	RAN=$(grep "coeff. of determination (aw)" $nfile \
		| grep '=' \
		| cut -d ' ' -f 6)
	RMN=$(grep "final cost" $nfile \
		| grep '=' \
		| cut -d ' ' -f 15)

	RPV=$(grep "coeff. of determination" $vfile \
		| grep -v "aw" \
		| grep '=' \
		| cut -d ' ' -f 6)
	RAV=$(grep "coeff. of determination (aw)" $vfile \
		| grep '=' \
		| cut -d ' ' -f 6)
	RMV=$(grep "final cost" $vfile \
		| grep '=' \
		| cut -d ' ' -f 15)

	RPU=$(grep "coeff. of determination" $ufile \
		| grep -v "aw" \
		| grep '=' \
		| cut -d ' ' -f 6)
	RAU=$(grep "coeff. of determination (aw)" $ufile \
		| grep '=' \
		| cut -d ' ' -f 6)
	RMU=$(grep "final cost" $ufile \
		| grep '=' \
		| cut -d ' ' -f 15)

	array=("$@")
	YEAR=$3
	SOURCE=$4
	AUTHORSHIP="$5"

	INDICATOR=$6

	grep "%${INDICATOR}$" $texfile \
		| sed "s/FIRST/${FIRST} /" \
		| sed "s/SECOND/${SECOND} /" \
		| sed "s/THIRD/${THIRD} /" \
		| sed "s/NK1/${NK1}/" \
		| sed "s/NK2/${NK2}/" \
		| sed "s/VB1/${VB1}/" \
		| sed "s/VB1/${VB2}/" \
		| sed "s/VC12/${VC12}/" \
		| sed "s/VB3/${VB3}/" \
		| sed "s/VC13/${VC13}/" \
		| sed "s/VC23/${VC23}/" \
		| sed "s/UA00/${UA00}/" \
		| sed "s/UA01/${UA01}/" \
		| sed "s/UA02/${UA02}/" \
		| sed "s/UA03/${UA03}/" \
		| sed "s/UA10/${UA10}/" \
		| sed "s/UA11/${UA11}/" \
		| sed "s/UA12/${UA12}/" \
		| sed "s/UA13/${UA13}/" \
		| sed "s/UA20/${UA20}/" \
		| sed "s/UA21/${UA21}/" \
		| sed "s/UA22/${UA22}/" \
		| sed "s/UA23/${UA23}/" \
		| sed "s/UA30/${UA30}/" \
		| sed "s/UA31/${UA31}/" \
		| sed "s/UA32/${UA32}/" \
		| sed "s/UA33/${UA33}/" \
		| sed "s/RPR/${RPR}/" \
		| sed "s/RAR/${RAR}/" \
		| sed "s/RMR/${RMR}/" \
		| sed "s/RPN/${RPN}/" \
		| sed "s/RAN/${RAN}/" \
		| sed "s/RMN/${RMN}/" \
		| sed "s/RPV/${RPV}/" \
		| sed "s/RAV/${RAV}/" \
		| sed "s/RMV/${RMV}/" \
		| sed "s/RPU/${RPU}/" \
		| sed "s/RAU/${RAU}/" \
		| sed "s/RMU/${RMU}/" \
		| sed "s/YEAR/${YEAR}/" \
		| sed "s/SOURCE/${SOURCE}/" \
		| sed "s/AUTHORSHIP/${AUTHORSHIP}/" \
		| sed "s/NAME/r_${NAME}/" \
		| sed "s/\ *%B//g" \
		| sed "s/\ *%T//g" \
		| sed "s/\ *%B//g"

	echo
}

for model in {raoult,norrish,virial,uniquac}
do
	for file in $(cat data/lists/mono_all.csv \
			data/lists/ternary_carbohydrates.txt \
			data/lists/quaternary_carbohydrates.txt)
	do
		tempK=$(echo $file \
			| sed 's/30_C/303_15_K/;s/35_C/308_15_K/' \
			| grep '_K' \
			| cut -d '_' -f 3- \
			| sed 's/_K\.csv//' \
			| rev \
			| cut -d '_' -f -2 \
			| tr '_' '.' | rev | sed 's/^/-T /')
		head -n1 $file > /tmp/${model}_$(basename -- $file)
		./bin/FitWaterActivity \
			-f $file \
			-F /tmp/file.csv \
			-m $model \
			-q -M 20000 $tempK >> /tmp/${model}_$(basename -- $file)
	done
done

